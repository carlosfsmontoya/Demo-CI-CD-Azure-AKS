---
interface Image {
  src: string;
  alt: string;
}

interface Props {
  images: Image[];
}

const { images } = Astro.props as Props;
---

<div class="flex carousel carousel-center rounded-box">
  {images.map((image) => (
    <div class="carousel-item">
      <img src={image.src} alt={image.alt} />
    </div>
  ))}
</div>